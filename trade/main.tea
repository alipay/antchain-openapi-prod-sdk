  // 商品主数据编码
  innerCode: string(name='inner_code', example='ZWZJFM00167122'),
  // 商品类型，资源包：attach
  type: string(name='type', example='attach'),
  // 付费方式，预付：PREPAY，按量付费：POSTPAY
