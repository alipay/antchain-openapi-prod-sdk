  akProxy?: string(name='ak_proxy', example='EcsConsole'),
  // API名称
  apiName?: string(name='api_name', example='CreateUser'),
  // 调用者IP
  // 
  appIp?: string(name='app_ip', example='127.0.0.1'),
  // 调用者BID
