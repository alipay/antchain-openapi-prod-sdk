/**
 * Description: 理赔材料照片分类、质检、增强
 * Summary: 理赔材料照片分类
 */
async function execImageClassification(request: ExecImageClassificationRequest): ExecImageClassificationResponse {
  var runtime = new Util.RuntimeOptions{};
  var headers : map[string]string= {};
  return execImageClassificationEx(request, headers, runtime);
}

/**
 * Description: 理赔材料照片分类、质检、增强
 * Summary: 理赔材料照片分类
 */
async function execImageClassificationEx(request: ExecImageClassificationRequest, headers: map[string]string, runtime: Util.RuntimeOptions): ExecImageClassificationResponse {
  if (!Util.isUnset(request.fileObject)) {
    var uploadReq = new CreateAntcloudGatewayxFileUploadRequest{
      authToken = request.authToken,
      apiCode = 'antdigital.aiclaim.image.classification.exec',
      fileName = request.fileObjectName,
    };
    var uploadResp = createAntcloudGatewayxFileUploadEx(uploadReq, headers, runtime);
    if (!AntchainUtil.isSuccess(uploadResp.resultCode, "ok")) {
      var execImageClassificationResponse = new ExecImageClassificationResponse{
        reqMsgId = uploadResp.reqMsgId,
        resultCode = uploadResp.resultCode,
        resultMsg = uploadResp.resultMsg,
      };
      return execImageClassificationResponse;
    }
    var uploadHeaders = AntchainUtil.parseUploadHeaders(uploadResp.uploadHeaders);
    AntchainUtil.putObject(request.fileObject, uploadHeaders, uploadResp.uploadUrl);
    request.fileId = uploadResp.fileId;
    request.fileObject = null;
  }
  Util.validateModel(request);
  return doRequest('1.0' ,'antdigital.aiclaim.image.classification.exec', 'HTTPS', 'POST', `/gateway.do`, request, headers, runtime);
}

