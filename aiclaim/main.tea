// Extraction
model Extraction {
  // 提取出的具体信息的基类，不同类型的影像材料，有不同的数据结构，下文会详细展开。不同的sub_type映射不同的BaseExtractionData子类。
  data?: BaseExtractionData(name='data', example=''),
  // 失败原因(失败时填写)，成功时为null
  failureReason?: string(name='failure_reason', example='category_not_supported'),
  // 提取状态: success/not_supported
  status?: string(name='status', example='not_supported'),
}

// doument
model Document {
  // 参考分类接口中返回的分类结果
  type?: string(name='type', example='MEDICAL_EXPENSE_INVOICE'),
  // 参考分类接口中返回的分类结果
  typeCn?: string(name='type_cn', example='医疗费用发票'),
  // 细分的分类结果
  subType?: string(name='sub_type', example='TARGET_INVOICE_OUTPATIENT'),
  // 细分的分类结果
  subTypeCn?: string(name='sub_type_cn', example='门诊发票'),
  // 参考Extraction参数
  extraction?: [ Extraction ](name='extraction', example=''),
  // 参考Page参数
  page?: [ Page ](name='page', example=''),
}

