}

// Page
model Page {
  // 边界框坐标[x1,y1,x2,y2]
  bbox?: [ long ](name='bbox', example='[0,0,999,999]'),
  // 图片文件名
  image?: string(name='image', example='检验报告.jpg'),
  // 页面索引
  pageIndex?: long(name='page_index', example='0'),
