  enhanceImage?: string(name='enhance_image'),
  // 是否模糊，返回0表示清晰，返回1表示模糊
  isBlur?: boolean(name='is_blur'),
  // 是否复印件，返回0表示⾮ 复印件，返回1表示复印件
  isCopyfile?: boolean(name='is_copyfile'),
  // 是否翻拍件，返回0表示⾮ 翻拍件，返回1表示翻拍件
  isRemakefile?: boolean(name='is_remakefile'),
  // 材料是否不完整（有遮 挡），返回0表示完整（⽆ 遮挡），返回1表示材料不完 整（有遮挡）
  isUncomplet?: boolean(name='is_uncomplet'),
  // 单张图片中是否包含2份及以上材料，0表示只有1份，1表示两份及以上
  isMultiMaterial?: boolean(name='is_multi_material'),
