/**
 * Description: 三方平台文件上传
 * Summary: 三方平台文件上传
 */
async function uploadEcarPlaformfileEx(request: UploadEcarPlaformfileRequest, headers: map[string]string, runtime: Util.RuntimeOptions): UploadEcarPlaformfileResponse {
  if (!Util.isUnset(request.fileObject)) {
    var uploadReq = new CreateAntcloudGatewayxFileUploadRequest{
      authToken = request.authToken,
      apiCode = 'antchain.carbon.ecar.plaformfile.upload',
      fileName = request.fileObjectName,
    };
    var uploadResp = createAntcloudGatewayxFileUploadEx(uploadReq, headers, runtime);
    if (!AntchainUtil.isSuccess(uploadResp.resultCode, "ok")) {
      var uploadEcarPlaformfileResponse = new UploadEcarPlaformfileResponse{
        reqMsgId = uploadResp.reqMsgId,
        resultCode = uploadResp.resultCode,
        resultMsg = uploadResp.resultMsg,
      };
      return uploadEcarPlaformfileResponse;
    }
    var uploadHeaders = AntchainUtil.parseUploadHeaders(uploadResp.uploadHeaders);
    AntchainUtil.putObject(request.fileObject, uploadHeaders, uploadResp.uploadUrl);
    request.fileId = uploadResp.fileId;
    request.fileObject = null;
  }
  Util.validateModel(request);
  return doRequest('1.0' ,'antchain.carbon.ecar.plaformfile.upload', 'HTTPS', 'POST', `/gateway.do`, request, headers, runtime);
}

